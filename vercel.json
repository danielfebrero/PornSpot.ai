{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "frontend/**": {
      "includeFiles": "frontend/**"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "turbo run build",
  "outputDirectory": "frontend/.next"
}
