{
  "name": "pornspot-ai-backend",
  "version": "1.0.0",
  "description": "Backend Lambda functions for pornspot.ai gallery",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias && npm run copy:assets",
    "copy:assets": "cp -r shared/utils/*.json dist/shared/utils/ 2>/dev/null || true && cp -r shared/instructions/*.txt dist/shared/instructions/ 2>/dev/null || true && cp -r shared/email_templates/ dist/shared/email_templates/ 2>/dev/null || true ",
    "build:watch": "tsc --watch",
    "build:sam": "npm run build && cd .. && sam build",
    "build:docker": "./build-docker-lambdas.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:ci": "jest --coverage --watchAll=false",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-ssm": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@aws-sdk/types": "^3.450.0",
    "@sendgrid/mail": "^8.1.5",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.0",
    "google-auth-library": "^9.6.3",
    "googleapis": "^134.0.0",
    "mime-types": "^2.1.35",
    "sharp": "^0.34.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "dynamodb-local": "^0.0.31",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "gallery",
    "typescript"
  ],
  "author": "pornspot.ai",
  "license": "MIT"
}
