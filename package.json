{
  "name": "pornspot-ai",
  "version": "1.0.0",
  "description": "A minimalist gallery application for showcasing art and photography",
  "private": true,
  "packageManager": "npm@10.0.0",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "build": "turbo build",
    "build:backend": "turbo build --filter=pornspot-ai-backend",
    "build:frontend": "turbo build --filter=pornspot-ai-frontend",
    "dev:backend": "npm run clean:sam && npm run build:backend && sam build && sam local start-api --port 3001 --docker-network pornspot-ai_local-network --env-vars backend/.env.local.json --skip-pull-image --warm-containers LAZY",
    "dev:backend:log": "npm run clean:sam && npm run build:backend && sam build && sam local start-api --port 3001 --docker-network pornspot-ai_local-network --env-vars backend/.env.local.json > backend.log 2>&1",
    "dev:backend:simple": "npm run clean:sam && npm run build:backend && sam build && sam local start-api --port 3001 --env-vars backend/.env.local.json",
    "stop:backend": "./scripts/stop-local-backend.sh",
    "fix:docker": "./scripts/fix-docker-sam.sh",
    "fix:network": "./scripts/fix-docker-network.sh",
    "clean:sam": "./scripts/cleanup-sam-containers.sh",
    "dev:frontend": "turbo dev --filter=pornspot-ai-frontend",
    "deploy:backend:prod": "export ENVIRONMENT=prod && ./scripts/deploy-backend.sh",
    "deploy:backend:stage": "export ENVIRONMENT=stage && ./scripts/deploy-backend.sh",
    "local:start": "docker-compose -f docker-compose.local.yml up -d",
    "local:stop": "docker-compose -f docker-compose.local.yml down",
    "local:clean": "docker-compose -f docker-compose.local.yml down -v",
    "local:init": "chmod +x scripts/init-local-aws.sh && ./scripts/init-local-aws.sh",
    "local:logs": "docker-compose -f docker-compose.local.yml logs -f localstack",
    "db:setup": "node scripts/create-dynamodb-table.js",
    "local:setup": "npm run local:init && npm run db:setup",
    "migrate:urls": "node scripts/migrate-urls-to-relative.js",
    "repair:thumbnails": "node scripts/repair-thumbnails.js",
    "cleanup:thumbnails:s3": "node scripts/cleanup-thumbnails-s3.js",
    "cleanup:thumbnails:db": "node scripts/cleanup-thumbnails-db.js",
    "cleanup:thumbnails:all": "node scripts/cleanup-thumbnails-all.js",
    "delete:comments": "node scripts/delete-all-comments.js",
    "copy:shared-types": "rm -rf frontend/src/types/shared-types/* backend/shared/shared-types/* && mkdir -p frontend/src/types/shared-types backend/shared/shared-types && cp -r shared-types/* frontend/src/types/shared-types && cp -r shared-types/* backend/shared/shared-types/",
    "deploy": "./scripts/deploy.sh",
    "deploy:guided": "./scripts/deploy.sh --guided",
    "sam:build": "sam build",
    "sam:deploy": "sam deploy",
    "sam:local": "npm run clean:sam && sam build && sam local start-api --port 3001 --docker-network pornspot-ai_local-network --env-vars backend/.env.local.json",
    "lint": "turbo lint",
    "lint:backend": "turbo lint --filter=pornspot-ai-backend",
    "lint:frontend": "turbo lint --filter=pornspot-ai-frontend",
    "lint:fix": "turbo lint:fix",
    "lint:fix:backend": "turbo lint:fix --filter=pornspot-ai-backend",
    "lint:fix:frontend": "turbo lint:fix --filter=pornspot-ai-frontend",
    "test": "turbo test",
    "test:all": "turbo test",
    "test:backend": "turbo test --filter=pornspot-ai-backend",
    "test:backend:unit": "turbo test:unit --filter=pornspot-ai-backend",
    "test:backend:integration": "turbo test:integration --filter=pornspot-ai-backend",
    "test:backend:coverage": "turbo test:coverage --filter=pornspot-ai-backend",
    "test:backend:all": "turbo test --filter=pornspot-ai-backend",
    "test:frontend": "turbo test --filter=pornspot-ai-frontend",
    "test:frontend:unit": "turbo test:unit --filter=pornspot-ai-frontend",
    "test:frontend:integration": "turbo test:integration --filter=pornspot-ai-frontend",
    "test:frontend:e2e": "turbo test:e2e --filter=pornspot-ai-frontend",
    "test:frontend:coverage": "turbo test:coverage --filter=pornspot-ai-frontend",
    "test:frontend:all": "turbo test --filter=pornspot-ai-frontend",
    "test:watch": "concurrently \"turbo test:watch --filter=pornspot-ai-backend\" \"turbo test:watch --filter=pornspot-ai-frontend\"",
    "test:backend:watch": "turbo test:watch --filter=pornspot-ai-backend",
    "test:frontend:watch": "turbo test:watch --filter=pornspot-ai-frontend",
    "test:coverage": "turbo test:coverage",
    "test:coverage:combined": "jest --config jest.config.root.js --coverage",
    "test:ci": "npm run test:backend:all && npm run test:frontend:all",
    "test:summary": "node scripts/generate-test-summary.js",
    "clean": "turbo clean",
    "clean:backend": "turbo clean --filter=pornspot-ai-backend",
    "clean:frontend": "turbo clean --filter=pornspot-ai-frontend",
    "type-check": "turbo type-check",
    "type-check:backend": "turbo type-check --filter=pornspot-ai-backend",
    "type-check:frontend": "turbo type-check --filter=pornspot-ai-frontend"
  },
  "keywords": [
    "gallery",
    "art",
    "photography",
    "serverless",
    "aws",
    "nextjs",
    "typescript",
    "minimalist"
  ],
  "author": "pornspot.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielfebrero/PornSpot.ai.git"
  },
  "bugs": {
    "url": "https://github.com/danielfebrero/PornSpot.ai/issues"
  },
  "homepage": "https://PornSpot.ai",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lcov-result-merger": "^3.3.0",
    "lint-staged": "^15.1.0",
    "turbo": "^2.5.5"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "styled-jsx": "^5.1.1"
  }
}
